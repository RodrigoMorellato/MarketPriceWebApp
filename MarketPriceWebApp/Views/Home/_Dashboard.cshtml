<div class="text-center">
    @model MarketViewModel

    <p>
        <button class="btn btn-primary collapsible" type="button" data-bs-toggle="collapse" data-bs-target="#collapseDashboard" aria-expanded="false" aria-controls="collapseDashboard">
            <h5>@ViewData["Title"]</h5>
        </button>
    </p>
    <div class="collapse" id="collapseDashboard">
        <div class="card card-body">
            <div>
                <div class="row">
                    <div id="marketPrices" style="width:100%; height:450px;"></div>
                </div>
            </div>
        </div>
    </div>
</div>
